<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="${packageName}">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application>
        <meta-data
            android:name="com.google.android.gms.car.application"
            android:resource="@xml/automotive_app_desc" />

        <service android:name="com.acnodelabs.apps.untitled.MyMessagingService"></service>

        <receiver android:name="com.acnodelabs.apps.untitled.MessageReadReceiver">
            <intent-filter>
                <action android:name="com.acnodelabs.apps.untitled.ACTION_MESSAGE_READ" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.acnodelabs.apps.untitled.MessageReplyReceiver">
            <intent-filter>
                <action android:name="com.acnodelabs.apps.untitled.ACTION_MESSAGE_REPLY" />
            </intent-filter>
        </receiver>
    </application>

</manifest>